{
	"name": "Quin69 Alert",
	"description": "quin69 extension",
	"version": "1.0",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"permissions": ["notifications", "alarms", "storage"],
	"host_permissions": ["http://localhost:3000/"],
	"action": {
		"default_icon": {
		  "32": "/images/painchamp32.png",
		  "64": "/images/painchamp64.png",
		  "128": "/images/painchamp128.png"
		  }
	},
	"icons": {
	  "32": "/images/painchamp32.png",
	  "64": "/images/painchamp64.png",
	  "128": "/images/painchamp128.png"
  }
}